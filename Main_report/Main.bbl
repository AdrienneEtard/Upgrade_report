    \verb 10.1073/pnas.1116791109
    \endverb
    \field{isbn}{0027-8424}
    \field{issn}{0027-8424}
    \field{title}{{Dispersal will limit ability of mammals to track climate
  change in the Western Hemisphere}}
    \field{journaltitle}{Proceedings of the National Academy of Sciences}
    \field{year}{2012}
