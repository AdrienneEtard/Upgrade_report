    \verb 10.1371/journal.pbio.2006841
    \endverb
    \field{issn}{15457885}
    \field{title}{{Widespread winners and narrow-ranged losers: Land use
  homogenizes biodiversity in local assemblages worldwide}}
    \field{journaltitle}{PLoS Biology}
    \field{year}{2018}
  \endentry

