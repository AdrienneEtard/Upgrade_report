    \endverb
    \field{isbn}{1365-2486}
    \field{issn}{13541013}
    \field{title}{{The fate of European breeding birds under climate, land-use
  and dispersal scenarios}}
    \field{journaltitle}{Global Change Biology}
    \field{year}{2012}
  \endentry

