    \endverb
    \field{isbn}{1111111111}
    \field{issn}{19326203}
    \field{title}{{Scientific research on animal biodiversity is systematically
  biased towards vertebrates and temperate regions}}
    \field{journaltitle}{PLoS ONE}
    \field{year}{2017}
  \endentry

