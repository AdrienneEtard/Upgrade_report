    \verb 10.1111/geb.12491
    \endverb
    \field{isbn}{1466-8238}
    \field{issn}{14668238}
    \field{title}{{Late Quaternary reptile extinctions: size matters,
  insularity dominates}}
    \field{journaltitle}{Global Ecology and Biogeography}
    \field{year}{2016}
  \endentry

