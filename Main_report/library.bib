publisher = {Public Library of Science},
title = {{Agricultural Management and Climatic Change Are the Major Drivers of Biodiversity Change in the UK.}},
url = {http://journals.plos.org/plosone/article?id=10.1371/journal.pone.0151595},
volume = {11},
year = {2016}
}
