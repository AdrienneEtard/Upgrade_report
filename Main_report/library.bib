keywords = {Base Sequence,Carbon Isotopes,Cell Culture Techniques,Chemical Fractionation,Euryarchaeota,Kinetics,Methane,Molecular Sequence Data,Pressure,Survival Analysis,Temperature,Thermodynamics},
number = {31},
pages = {10949--10954},
pmid = {18664583},
title = {{Cell proliferation at 122 degrees C and isotopically heavy CH4 production by a hyperthermophilic methanogen under high-pressure cultivation}},
url = {papers://0be24a46-325a-4116-a3c6-fd8a3b614472/Paper/p11728},
