journal = {Annual Review of Phytopathology},
number = {1},
pages = {489--509},
pmid = {16722808},
title = {{Climate Change Effects on Plant Disease: Genomes to Ecosystems}},
url = {http://www.annualreviews.org/doi/10.1146/annurev.phyto.44.070505.143420},
volume = {44},
