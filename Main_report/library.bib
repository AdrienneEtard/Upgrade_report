journal = {Functional Ecology},
keywords = {Carbon fixation,Cryptobiotic crust,Cryptogamic crust,Water relations},
number = {2},
pages = {195--202},
title = {{Photosynthesis of the cyanobacterial soil-crust lichen Collema tenax from arid lands in southern Utah, USA: Role of water content on light and temperature responses of CO2 exchange}},
volume = {12},
year = {1998}
}
