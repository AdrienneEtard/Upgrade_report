issn = {15371719},
journal = {Molecular Biology and Evolution},
keywords = {biodiversity,diversification,speciation,timetree,tree of life},
pmid = {25739733},
title = {{Tree of life reveals clock-like speciation and diversification}},
year = {2015}
}
