journal = {Annual Review of Phytopathology},
keywords = {a single atmospheric constituent,been lim-,climate variability,epidemiological models,global warming,host-pathogen interactions,impacts of climate change,ited,on plant diseases has,on the effects of,risk and impact assessment,s abstract research on,with most work concentrating},
number = {1},
pages = {399--426},
pmid = {11701829},
title = {{Climate change and plant disease management}},
url = {http://www.annualreviews.org/doi/abs/10.1146/annurev.phyto.37.1.399},
volume = {37},
year = {1999}
}
