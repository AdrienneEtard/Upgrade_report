keywords = {Enzymatic reaction,Modeling,Oligosaccharides,Temperature,$\beta$-galactosidase},
month = {feb},
number = {2–4},
pages = {271--281},
title = {{Effect of temperature and enzyme origin on the enzymatic synthesis of oligosaccharides}},
url = {http://www.sciencedirect.com/science/article/pii/S0141022999001672},
volume = {26},
year = {2000}
}
