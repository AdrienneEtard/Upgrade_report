journal = {Industrial Crops and Products},
number = {1},
pages = {192--198},
publisher = {Elsevier B.V.},
title = {{Quantification of the cardinal temperatures and thermal time requirement of opium poppy (Papaver somniferum L.) seeds to germinate using non-linear regression models}},
url = {http://dx.doi.org/10.1016/j.indcrop.2011.06.033},
volume = {35},
year = {2012}
}
@article{Saranraj2013,
