journal = {Science (New York, N.Y.)},
keywords = {Altitude,Animal,Animals,Behavior,Climate Change,Climate change,Ecosystem,Environment,Geography,Population Dynamics,Species Specificity,Time Factors},
mendeley-tags = {Climate change},
