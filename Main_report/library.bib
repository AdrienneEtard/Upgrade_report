booktitle = {Oikos},
doi = {10.1034/j.1600-0706.2003.12378.x},
issn = {00301299},
title = {{Scavenging by vertebrates: Behavioral, ecological, and evolutionary perspectives on an important energy transfer pathway in terrestrial ecosystems}},
